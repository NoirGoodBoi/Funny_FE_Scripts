--[[beta version of new update for pc:
loadstring(game:HttpGet("https://raw.githubusercontent.com/396abc/Script/refs/heads/main/FlytestBeta.lua"))()
enjoy pls]]--
local UserInputService = game:GetService("UserInputService")

local isMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled

if isMobile then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/396abc/Script/refs/heads/main/MobileFly.lua"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/396abc/Script/refs/heads/main/FlyR15.lua"))()
end
